{
  "name": "pants-control",
  "version": "0.1.0",
  "scripts": {
    "app": "cross-env BROWSER=none react-app-rewired start",
    "web": "yarn clear:babel-cache && react-app-rewired start",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 ELECTRON_DEV=1 electron .",
    "start": "yarn clear:babel-cache && concurrently \"yarn app\" \"wait-on http://localhost:3000/ && yarn electron\"",
    "build": "yarn clear:babel-cache && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "pm2": "pm2 delete 0 && pm2 start yarn -- start",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "postinstall": "cd ./src && cross-env npm_config_target=1.8.2 npm_config_arch=x64 npm_config_target_arch=x64 npm_config_disturl=https://atom.io/download/electron npm_config_runtime=electron npm_config_build_from_source=true yarn",
    "eject": "react-scripts eject"
  },
  "private": true,
  "dependencies": {
    "@manaflair/redux-batch": "^0.1.0",
    "aes-js": "^3.1.0",
    "antd": "^3.3.1",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link-http": "^1.5.3",
    "apollo-server-express": "^1.3.2",
    "async-get-json": "^4.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "body-parser": "^1.18.2",
    "buffer": "^5.1.0",
    "electron": "^1.8.4",
    "electron-json-storage": "^4.0.2",
    "electron-remote": "^1.2.0",
    "electron-window": "^0.8.1",
    "electron-window-state": "^4.1.1",
    "express": "^4.16.3",
    "fix-path": "^2.1.0",
    "flow-runtime": "^0.17.0",
    "fs-extra": "^5.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.23.1",
    "history": "^4.7.2",
    "immer": "^1.2.1",
    "immutable": "^3.8.2",
    "ipfs-api": "^18.2.1",
    "is-electron": "^2.1.0",
    "is-electron-renderer": "^2.0.1",
    "keypair": "^1.0.1",
    "lodash": "^4.17.5",
    "preval.macro": "^1.0.2",
    "react": "^16.2.0",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "rebass": "^1.0.7",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "redux-saga": "^0.16.0",
    "redux-saga-routines": "^2.2.2",
    "rxjs": "^5.5.7",
    "scrypt-async-modern": "^3.0.6",
    "seamless-immutable": "^7.1.3",
    "segmentit": "^1.1.4",
    "slate": "^0.33.3",
    "slate-react": "^0.12.3",
    "stream-buffers": "^3.0.1",
    "string-to-arraybuffer": "^1.0.0",
    "styled-components": "^3.2.3",
    "styled-flex-component": "^2.2.2",
    "styled-is": "^1.1.2",
    "styled-normalize": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-macros": "^2.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "electron-is-dev": "^0.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "wait-on": "^2.1.0"
  }
}

{
  "name": "pants-control",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "export ELECTRON_MIRROR=\"https://npm.taobao.org/mirrors/electron/\"",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "app": "cross-env BROWSER=none react-app-rewired start",
    "web": "yarn clear:babel-cache && react-app-rewired start",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 ELECTRON_DEV=1 electron .",
    "start": "yarn clear:babel-cache && concurrently \"yarn app\" \"wait-on http://localhost:3000/ && yarn electron\"",
    "build": "yarn clear:babel-cache && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "pm2": "pm2 delete 0 && pm2 start yarn -- start",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "postinstall": "cd ./src && cross-env npm_config_target=1.8.2 npm_config_arch=x64 npm_config_target_arch=x64 npm_config_disturl=https://atom.io/download/electron npm_config_runtime=electron npm_config_build_from_source=true yarn",
    "eject": "react-scripts eject"
  },
  "private": true,
  "dependencies": {
    "@manaflair/redux-batch": "^0.1.0",
    "@rematch/core": "^1.0.0-alpha.4",
    "@rematch/immer": "^1.0.2",
    "@rematch/select": "0.3.0",
    "aes-js": "^3.1.1",
    "antd": "^3.5.2",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link-http": "^1.5.4",
    "apollo-server-express": "^1.3.6",
    "babel-plugin-styled-components": "^1.5.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "buffer": "^5.1.0",
    "electron": "^2.0.1",
    "electron-json-storage": "^4.1.0",
    "electron-remote": "^1.2.0",
    "electron-window": "^0.8.1",
    "electron-window-state": "^4.1.1",
    "express": "^4.16.3",
    "fix-path": "^2.1.0",
    "flow-runtime": "^0.17.0",
    "fs-extra": "^6.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^3.0.1",
    "history": "^4.7.2",
    "ipfs-api": "^21.0.0",
    "is-electron": "^2.1.0",
    "is-electron-renderer": "^2.0.1",
    "keypair": "^1.0.1",
    "level-js": "^2.2.4",
    "lodash": "^4.17.10",
    "lru-cache": "^4.1.3",
    "node-forge": "^0.7.5",
    "preval.macro": "^1.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-scripts": "^2.0.0-next.66cc7a90",
    "redux": "^4.0.0",
    "scrypt-async-modern": "^3.0.6",
    "segmentit": "^1.1.4",
    "slate": "^0.33.6",
    "slate-plain-serializer": "^0.5.12",
    "slate-react": "^0.12.6",
    "stream-buffers": "^3.0.1",
    "string-to-arraybuffer": "^1.0.0",
    "styled-components": "^3.2.6",
    "styled-flex-component": "^2.2.2",
    "styled-is": "^1.1.3",
    "styled-normalize": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "devDependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.46",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.47",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.46",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-macros": "^2.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
    "cross-spawn": "^6.0.5",
    "electron-is-dev": "^0.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0",
    "react-app-rewired": "^1.5.2",
    "react-cosmos": "^4.3.0",
    "spawn-sync": "^2.0.0",
    "wait-on": "^2.1.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}

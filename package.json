{
  "name": "pants-control",
  "version": "0.1.0",
  "scripts": {
    "react": "react-scripts start",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 ELECTRON_DEV=1 electron .",
    "start": "concurrently \"yarn react\" \"wait-on http://localhost:3000/ && yarn electron-dev\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "pm2": "pm2 delete 0 && pm2 start yarn -- start",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "eject": "react-scripts eject"
  },
  "private": true,
  "dependencies": {
    "@manaflair/redux-batch": "^0.1.0",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.1",
    "apollo-link-http": "^1.3.2",
    "async-get-json": "^4.0.0",
    "electron-json-storage": "^4.0.2",
    "fix-path": "^2.1.0",
    "fs-extra": "^5.0.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-saga-routines": "^2.2.2",
    "segmentit": "^1.1.4",
    "slate": "^0.32.2",
    "slate-react": "^0.11.2",
    "uuid": "^3.2.1"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^1.7.11",
    "wait-on": "^2.1.0"
  }
}

{
  "name": "pants-control",
  "version": "0.1.0",
  "scripts": {
    "app": "cross-env BROWSER=none react-app-rewired start",
    "web": "react-scripts start",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 ELECTRON_DEV=1 electron .",
    "start": "concurrently \"yarn app\" \"wait-on http://localhost:3000/ && yarn electron\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "pm2": "pm2 delete 0 && pm2 start yarn -- start",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "eject": "react-scripts eject"
  },
  "private": true,
  "dependencies": {
    "@manaflair/redux-batch": "^0.1.0",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.1",
    "apollo-link-http": "^1.3.2",
    "apollo-server-express": "^1.3.2",
    "async-get-json": "^4.0.0",
    "body-parser": "^1.18.2",
    "electron-is": "^0.3.0",
    "electron-json-storage": "^4.0.2",
    "electron-window": "^0.8.1",
    "electron-window-state": "^4.1.1",
    "emotion": "^8.0.12",
    "express": "^4.16.2",
    "fix-path": "^2.1.0",
    "fs-extra": "^5.0.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.19.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "is-electron-renderer": "^2.0.1",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-app-rewired": "^1.4.1",
    "react-dom": "^16.2.0",
    "react-emotion": "^8.0.12",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-saga-routines": "^2.2.2",
    "segmentit": "^1.1.4",
    "slate": "^0.32.2",
    "slate-react": "^0.11.2",
    "uuid": "^3.2.1"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-macros": "^2.1.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^1.7.11",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.3.0",
    "wait-on": "^2.1.0"
  }
}
